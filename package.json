{
  "name": "hypergiant",
  "version": "3.0.2",
  "description": "Hypergiant is a small and simple signal-like event emitter for Node.js and the browser.",
  "main": "index.js",
  "module": "hypergiant.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "bundle": "rollup -c",
    "bundle:watch": "rollup -c --watch",
    "test": "mocha",
    "bundle:build": "npm run bundle && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:robertcorponoi/hypergiant"
  },
  "keywords": [
    "signal",
    "signals",
    "event",
    "events",
    "emit",
    "emitter"
  ],
  "typings": "lib/index.d.ts",
  "author": "Robert Corponoi",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sinon": "^8.0.4",
    "typescript": "^3.7.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7"
  }
}
